  #set($mediaParameter[$zerocount] = $item.trim())
#end
#set($emailImg = $xcontext.getURLFactory().createAttachmentURL("email.png", "MediaShare", "ShareButtonsMacro", "download", "", "$doc.getWiki()", $context.context))
#set($facebookImg = $xcontext.getURLFactory().createAttachmentURL("facebook.png", "MediaShare", "ShareButtonsMacro", "download", "", "$doc.getWiki()", $context.context))
#set($googleImg = $xcontext.getURLFactory().createAttachmentURL("google.png", "MediaShare", "ShareButtonsMacro", "download", "", "$doc.getWiki()", $context.context))
#set($linkedinImg = $xcontext.getURLFactory().createAttachmentURL("linkedin.png", "MediaShare", "ShareButtonsMacro", "download", "", "$doc.getWiki()", $context.context))
#set($pinterestImg = $xcontext.getURLFactory().createAttachmentURL("pinterest.png", "MediaShare", "ShareButtonsMacro", "download", "", "$doc.getWiki()", $context.context))
#set($twitterImg = $xcontext.getURLFactory().createAttachmentURL("twitter.png", "MediaShare", "ShareButtonsMacro", "download", "", "$doc.getWiki()", $context.context))
#set($printImg = $xcontext.getURLFactory().createAttachmentURL("print.png", "MediaShare", "ShareButtonsMacro", "download", "", "$doc.getWiki()", $context.context))
#set($pinterestMedia = $xcontext.macro.params.pinterestMedia)

{{html clean="false"}}

&lt;div id="share-buttons"&gt;
#if($mediaParameter.contains('email') || $mediaParameter.contains('all'))
  &lt;!-- Email --&gt;
  #set($mailSubject = $xcontext.macro.params.mailContent)
  &lt;a href="mailto:?Subject=$!{doc.title}&amp;amp;Body=$!{mailSubject} $!{escapetool.url($doc.getExternalURL())}"&gt;
    &lt;img src="$!{emailImg}" alt="Email" /&gt;
  &lt;/a&gt;
  #end
  #if($mediaParameter.contains('facebook') || $mediaParameter.contains('all'))
  &lt;!-- Facebook documented at https://developers.facebook.com/docs/plugins/share-button/--&gt;
  &lt;a href="http://www.facebook.com/sharer.php?u=$!{escapetool.url($doc.getExternalURL())}" target="_blank"&gt;
    &lt;img src="$!{facebookImg}" alt="Facebook" /&gt;
  &lt;/a&gt;
  #end
  #if($mediaParameter.contains('googleplus') || $mediaParameter.contains('all'))
  &lt;!-- Google+ documented at https://developers.google.com/+/web/share/#sharelink --&gt;
  &lt;a href="https://plus.google.com/share?url=$!{escapetool.url($doc.getExternalURL())}" target="_blank"&gt;
    &lt;img src="$!{googleImg}" alt="Google" /&gt;
  &lt;/a&gt;
  #end
  #if($mediaParameter.contains('linkedin') || $mediaParameter.contains('all'))
  &lt;!-- LinkedIn documented at https://developer.linkedin.com/docs/share-on-linkedin --&gt;
  &lt;a href="http://www.linkedin.com/shareArticle?mini=true&amp;amp;url=$!{escapetool.url($doc.getExternalURL())}" target="_blank"&gt;
    &lt;img src="$!{linkedinImg}" alt="LinkedIn" /&gt;
  &lt;/a&gt;
  #end
  #if($mediaParameter.contains('pinterest') || $mediaParameter.contains('all'))
  &lt;!-- Pinterest documentation from https://developers.pinterest.com/docs/widgets/save/ and button code and js from https://developers.pinterest.com/tools/widget-builder/?type=pinit&amp;round=false&amp;tall=false&amp;buttonType=anyImage--&gt;
  &lt;a data-pin-do="buttonBookmark" data-pin-save="true" data-pin-custom='true'  href="http://pinterest.com/pin/create/button/?url=$!{escapetool.url($doc.getExternalURL())}&amp;media=$!{pinterestMedia}&amp;description=$!{doc.title}" target="_blank"&gt;
    &lt;img src="$!{pinterestImg}" alt="Pinterest" /&gt;
  &lt;/a&gt;
  ##&lt;script async defer src="//assets.pinterest.com/js/pinit.js"&gt;&lt;/script&gt;
  #end
  #if($mediaParameter.contains('twitter') || $mediaParameter.contains('all'))
  &lt;!-- Twitter documentation from https://dev.twitter.com/web/tweet-button/web-intent --&gt;
  &lt;a href="https://twitter.com/share?url=$!{escapetool.url($doc.getExternalURL())};text=$!{xcontext.macro.params.twitterText};hashtags=$!{xcontext.macro.params.hashtags}" target="_blank"&gt;
    &lt;img src="$!{twitterImg}" alt="Twitter" /&gt;
  &lt;/a&gt;
  #end
  #if($mediaParameter.contains('print') || $mediaParameter.contains('all'))
  &lt;!-- Print --&gt;
  &lt;form method="get" action="$doc.getURL('export')" class="xformInline" style="display: inline;"&gt;
    &lt;input type="hidden" name="format" value="pdf" /&gt;
    &lt;input type="hidden" name="pdfcover" value="0" /&gt;
    &lt;input type="hidden" name="pdftoc" value="0" /&gt;
    &lt;input type="hidden" name="comments" value="0" /&gt;
    &lt;input type="hidden" name="attachments" value="0" /&gt;
    &lt;span class="stButton"&gt;
      &lt;input type="image" src="$!{printImg}" alt="pdfexport" class="stLarge" /&gt;
    &lt;/span&gt;
  &lt;/form&gt;     
  #end
&lt;/div&gt;
{{/html}}
{{/velocity}}</code>
